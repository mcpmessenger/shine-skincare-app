{
  "frontend": {
    "artifacts": {
      "baseDirectory": ".next",
      "files": [
        "**/*"
      ]
    },
    "cache": {
      "paths": [
        "node_modules/**/*",
        ".next/cache/**/*",
        ".pnpm-store/**/*"
      ]
    },
    "phases": {
      "build": {
        "commands": [
          "pnpm run build"
        ]
      },
      "preBuild": {
        "commands": [
          "npm install -g pnpm",
          "pnpm install --no-frozen-lockfile"
        ]
      }
    }
  }
}